#include <stdio.h>

int main() {
  float celsius, fahrenheit;

  printf("Por favor, introduce la temperatura en grados Celsius: ");
  scanf("%f", &celsius);

  fahrenheit = (celsius * 9.0 / 5.0) + 32.0;

  printf("%.2f grados Celsius son equivalentes a %.2f grados Fahrenheit.\n", celsius, fahrenheit);

  return 0;
}
